}\n            />\n          </Route>\n          <Route exact path=\"/login\">\n            <Login setIsLogin={setIsLogin} setAccessToken={setAccessToken} inputMyInfo={inputMyInfo} />\n          </Route>\n          <Route exact path=\"/register\" component={Register}/>\n          <Route exact path=\"/search\" component={Search} />\n          <Route exact path=\"/write\">\n            <Post\n              userData={userData}\n              postData={postData}\n              setPostData={setPostData}\n            />\n          </Route>\n        </Switch>\n      </Router>\n    </>\n  );\n};\n\nexport default App;\n","/home/kim/codestates/first-project/record-client/client/src/reportWebVitals.js",[],"/home/kim/codestates/first-project/record-client/client/src/schema/user.js",[],"/home/kim/codestates/first-project/record-client/client/src/schema/post.js",[],"/home/kim/codestates/first-project/record-client/client/src/components/Search/Search.jsx",["69"],"import React, { useState } from 'react';\nimport styles from '../Search/Search.module.css';\nimport Navbar from '../Navbar/Navbar';\nconst Search = (props) => {\n  const [search, setSearch] = useState('');\n\n  const searchInputHandler = (event) => {\n    setSearch(event.currentTarget.value);\n    /* console.log(search); */\n  };\n  return (\n    <div className={styles.container}>\n      <Navbar />\n      <div className={styles.inputContainer}>\n        <div className={styles.inputs}>\n          <input\n            onChange={searchInputHandler}\n            placeholder=\"검색어를 입력하세요\"\n          />\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default Search;\n","/home/kim/codestates/first-project/record-client/client/src/components/Register/Register.jsx",["70"],"/home/kim/codestates/first-project/record-client/client/src/components/LandingPage/LandingPage.jsx",[],"/home/kim/codestates/first-project/record-client/client/src/components/Login/Login.jsx",[],"/home/kim/codestates/first-project/record-client/cl